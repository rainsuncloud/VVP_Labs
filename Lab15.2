#include <stdio.h>

int Sign(float X)
{
    int p;
    if (X < 0) {
        p = -1;
    }
    else if (X = 0) {
        p = 0;
    }
    else if (X > 0) {
        p = 1;
    }
    return p;
}

int main()
{
    float a, b;

    printf("a = ");
    scanf("%f", &a);
    printf("b = ");
    scanf("%f", &b);
    
    printf("Sign(A) + Sign(B) = %d", Sign(a) + Sign(b));

    return 0;
}
