#include <stdio.h>
#include <locale.h>

int Quarter(float x, float y)
{
    int ch;
    if ((x > 0) && (y > 0)) {
        ch = 1;
    }
    else if ((x < 0) && (y > 0)) {
        ch = 2;
    }
    else if ((x < 0) && (y < 0)) {
        ch = 3;
    }
    else if ((x > 0) && (y < 0)) {
        ch = 4;
    }
    return ch;
}

int main()
{
    setlocale(LC_ALL, "Rus");

    float x1, y1, x2, y2, x3, y3;

    printf("x1 = ");
    scanf("%f", &x1);
    printf("x2 = ");
    scanf("%f", &x2);
    printf("x3 = ");
    scanf("%f", &x3);
    printf("y1 = ");
    scanf("%f", &y1);
    printf("y2 = ");
    scanf("%f", &y2);
    printf("y3 = ");
    scanf("%f", &y3);
    
    if ((y1 != 0) && (y2 != 0) && (y3 != 0) && (x1 != 0) && (x2 != 0) && (x3 != 0)) {
        
        printf("1 точка - %d четверть\n2 точка - %d четверть\n3 точка - %d четверть", Quarter(x1, y1), Quarter(x2, y2), Quarter(x3, y3));
    }
    else printf("Необходимо ввести числа не равные 0");
    
    return 0;
}
