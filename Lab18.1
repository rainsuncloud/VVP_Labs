#include <stdio.h>
#include <locale.h>

int main()
{
    setlocale(LC_ALL, "Rus");

    int n, k;

    printf("N = ");
    scanf("%d", &n);
    
    if (n > 0) {
        int a[n];
        int b[n];
        for (int i = 0; i < n; i++) {
            printf("a[%d] = ", i + 1);
            scanf("%d", &a[i]);
	    }
	    for (int i = 0; i < n; i++) {
            printf("b[%d] = ", i + 1);
            scanf("%d", &b[i]);
	    }
	    for (int i = 0; i < n; i++) {
	        k = a[i];
	        a[i] = b[i];
	        b[i] = k;
	    }
	    printf("Массив A:\n");
	    for (int i = 0; i < n; i++) {
            printf("%d, ", a[i]);
	    }
	    printf("\nМассив B:\n");
	    for (int i = 0; i < n; i++) {
            printf("%d, ", b[i]);
	    }
    }
    else printf("Необходимо ввести N больше 0");
    
    return 0;
}
