#include <stdio.h>
#include <locale.h>

int main()
{
    setlocale(LC_ALL, "Rus");

    int a, b;

    printf("a = ");
    scanf("%d", &a);
    printf("b = ");
    scanf("%d", &b);
    
    if ((a > 0) && (b > 0)) {
        while (a != b) {
            if (a > b) {
                a -= b;
            }
            else
                b -= a;
        }
        printf("НОД = %d", a);
    }
    else
    printf("Необходимо ввести целые положительные числа");
    return 0;
}
