#include <stdio.h>
#include <locale.h>

int main()
{
    setlocale(LC_ALL, "Rus");

    int n;

    printf("N = ");
    scanf("%d", &n);
    
    if (n > 0) {
        int a[n];
        for (int j = 0; j < n; j++) {
            printf("a[%d] = ", j + 1);
            scanf("%d", &a[j]);
	    }
	    int p = a[0];
        int i = 1;
        while (p > a[i]) {
            a[i - 1] = a[i];
            i++;
        }
        a[i - 1] = p;
        
	    for (int i = 0; i < n; i++) {
                printf("%d, ", a[i]);
	        }
    }
    else printf("Необходимо ввести N больше 0");
    
    return 0;
}
