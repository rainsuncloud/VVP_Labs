#include <stdio.h>
#include <locale.h>
#include <math.h>

int main()
{
    setlocale(LC_ALL, "Rus");

    int a, b;

    printf("A = ");
    scanf("%d", &a);
    printf("B = ");
    scanf("%d", &b);
    
    if ((a > b) && (a > 0) && (b > 0)) {
        for (b; b <= a; b++) {
            for (int i = 0; i < b; i++) {
                printf("%d", b);
            }
        }
    }
    else
    printf("Необходимо ввести целые положительные числа, причем А > B");
    return 0;
}
